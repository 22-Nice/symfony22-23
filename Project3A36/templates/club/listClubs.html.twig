<table>
<tr>
<th>Id</th>
<th>Name</th>
<th> Creation Date</th>
</tr>
{% for c in clubs %}
<tr>
<td>{{c.id}}</td>
<td>{{c.name}}</td>
<td>{{c.creationDate|date}}</td>
<td><a href="{{path('app_club_get',{'id':c.id})}}">Details</a></td>
<td><a href="{{path('app_club_remove',{'id':c.id})}}">Remove</a></td>
</tr>
{% endfor %}
</table>